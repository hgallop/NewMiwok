<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.newmiwok">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.android.newmiwok.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:parentActivityName="com.example.android.newmiwok.MainActivity" android:name="com.example.android.newmiwok.NumbersActivity" android:label="@string/category_numbers" />
        <activity android:parentActivityName="com.example.android.newmiwok.MainActivity" android:name="com.example.android.newmiwok.PhrasesActivity" android:label="@string/category_phrases"/>
        <activity android:parentActivityName="com.example.android.newmiwok.MainActivity" android:name="com.example.android.newmiwok.ColorsActivity" android:label="@string/category_colors"/>
        <activity android:parentActivityName="com.example.android.newmiwok.MainActivity" android:name="com.example.android.newmiwok.FamilyMembersActivity" android:label="@string/category_family"></activity>

        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value="com.example.android.newmiwok.MainActivity" />
    </application>

</manifest>